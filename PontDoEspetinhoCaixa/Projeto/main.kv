#####     TELA PRINCIPAL    #####
<Principal>:
    Screen:
        #: import hex kivy.utils.get_color_from_hex
        name: "Entrada"
        orientation: "horizontal"
        FloatLayout:
            Image: #Fundo legal
                source: "img/fundo.jpg"
                opacity: .5

            Image: #Logo do Point do Espetinho
                source: "img/logotipo.png"
                pos_hint: {"center_x": .7, "center_y": .5 }

            Label:
                text: "ENTRADA"
                font_size: 50
                size_hint: 0, .2
                pos_hint: {"x":.5, "top":1}
                color: hex("#4c2406")
                bold: True

            TextInput:
                font_size: 30
                size_hint: .25, .09
                pos_hint: {"center_x": .3, "center_y": .6}
                multiline: False
                hint_text: "Usuario"
                padding_y: 15
                padding_x: 10
                write_tab: False
                multiline: False
                input_filter: None
                id: user

            TextInput:
                font_size: 30
                size_hint: .25, .09
                pos_hint: {"center_x": .3, "center_y": .5}
                hint_text: "Senha"
                padding_y: 15
                padding_x: 10
                write_tab: False
                multiline: False
                password: True
                input_filter: None
                id: senha

            Button:
                text: "LOGIN"
                color: hex("#e9e9bb")
                bold: True
                font_size: 30
                size_hint: .25, .09
                pos_hint: {"center_x": .3, "center_y": .4}
                background_color: hex("#4c2406")
                background_normal: ""
                id: botaoLogin
                on_press: root.click_login()

            ActionBar:
                ActionView:
                    ActionPrevious:
                        on_press: app.stop()
                        title: "Sair"
                        background_color: hex("#4c2406")

                    ActionButton:
                        icon: "img/suporte.png"
                        title: "Suporte"
                        on_press: app.root.current = "suporte"
                    ActionSeparator:
                    ActionButton:
                        icon: "img/info.png"
                        title: "Sobre"
                        on_press: app.root.current = "info"


    AdmApp:
        name: 'Adm'
    ###############
    CaixaApp:
        name: 'Caixa'
    ###############
    FazerPedidoApp:
        name: 'Fazer_Pedido'
    PedidosApp:
        name: 'Pedidos'
    PedidoAltDelApp:
        name: "PedidoAltDel"
    ################
    CardapioApp:
        name: 'Cardapio'
    CardapioAddApp:
        name: 'CardapioAdd'
    CardapioAltDelApp:
        name: 'CardapioAltDel'
    ###############
    FuncApp:
        name: 'Func'
    CadastrarFunc:
        name: 'Cadastrar_Func'
    Funcionarios:
        name: 'Funcionarios'
    ################
    ClienteApp:
        name: 'Cliente'
    CadastrarCliente:
        name: 'Cadastrar_Cliente'
    Clientes:
        name: 'Clientes'
    #################
    RelatorioApp:
        name: 'Relatorio'
    #################
    EstoqueApp:
        name: 'Estoque'
    EstoqueProdutosAdicionarApp:
        name: 'EstoqueProdutosAdicionar'
    EstoqueProdutosAltDelApp:
        name: 'EstoqueProdutosAltDel'
    EstoqueProdutosVisualizarApp:
        name: 'EstoqueProdutosVisualizar'
    ################
    Suporte:
        name: 'suporte'
    Info:
        name: 'info'

#####     PAINEL ADM     #####
<AdmApp>:
    name: "Adm"
